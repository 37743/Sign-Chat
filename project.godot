; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="SignChat"
run/main_scene="res://MainUI/Splashscreen.tscn"
boot_splash/image="res://Assets/bootsplash.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://icon.png"

[autoload]

Global="*res://Effects/Audio.tscn"
autovar="*res://Scripts/autovar.gd"

[debug]

settings/fps/force_fps=60
settings/stdout/print_fps=true
shapes/collision/shape_color=Color( 0, 0.6, 0.7, 0 )

[display]

window/size/width=720
window/size/height=1520
window/size/test_width=297
window/size/test_height=600
window/dpi/allow_hidpi=true
window/energy_saving/keep_screen_on=false
window/handheld/orientation="portrait"
window/stretch/mode="viewport"
window/stretch/shrink="1"
window/handheld/emulate_touchscreen=true

[input]

Enter={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":16777221,"unicode":0,"echo":false,"script":null)
 ]
}

[memory]

limits/message_queue/max_size_kb=10024

[rendering]

quality/intended_usage/framebuffer_allocation=1
quality/intended_usage/framebuffer_allocation.mobile=1
environment/default_clear_color=Color( 0.168404, 0.15918, 0.25, 1 )
quality/filters/msaa=2
environment/default_environment="res://default_env.tres"
